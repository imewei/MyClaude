{
  "name": "slack-integration",
  "version": "1.0.0",
  "type": "integration",
  "description": "Send notifications to Slack after command execution",
  "author": "Claude Code Framework",
  "framework_version": ">=2.0.0",
  "python_version": ">=3.8",
  "dependencies": ["requests"],
  "capabilities": ["notifications", "integrations"],
  "supported_commands": ["all"],
  "config_schema": {
    "webhook_url": {"type": "string", "required": true},
    "channel": {"type": "string", "required": false},
    "username": {"type": "string", "required": false}
  },
  "default_config": {
    "username": "Claude Code Bot",
    "notify_success": true,
    "notify_failure": true
  },
  "permissions": ["network"],
  "sandbox": false,
  "license": "MIT",
  "tags": ["integration", "slack", "notifications"]
}